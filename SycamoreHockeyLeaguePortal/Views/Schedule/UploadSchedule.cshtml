@using SycamoreHockeyLeaguePortal.Models.InputForms
@model Schedule_UploadForm

@{
    ViewBag.Title = "Upload Schedule";
}

<h1>@ViewBag.Title</h1>

<form asp-action="UploadSchedule" method="post" enctype="multipart/form-data">
    <input type="hidden" asp-for="Season" />
    <fieldset class="form-group">
        <label asp-for="File"></label>
        <input type="file" asp-for="File" class="form-control" />
    </fieldset>
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Upload</button>
    </div>
</form>